import java.util.*;

public class forloops {
	 
	public static void main(String[] args){
		double counter = 0 ;	
		for(counter = 0; counter <= 10; counter += 2.3){
			System.out.println(counter);
		
			if(counter < 10){
			
				System.out.println("You haven't made it to 10 yet");
				
			 if(counter > 9 && counter < 10)
				
				System.out.println(" You almost made it to 10, but it is possible that the steps you are taking will NOT allow for you to reach 10. ");
			
			}
			
			 else {
				 System.out.println("It took " + counter/2 + " times to reach 10");
			} 
			}		
		
	}
		
}

